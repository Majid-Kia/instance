{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.2.3",
    "generators": {
      "api": {
        "generatorName": "typescript-axios",
        "inputSpec": "https://api.abrnoc.com/openapi.json",
        "output": "./src/api",
        "skipValidateSpec": true,
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
